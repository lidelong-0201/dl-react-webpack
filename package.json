    "sass-loader": "^14.0.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.1",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier": "^8.0.2",
